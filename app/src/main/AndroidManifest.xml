<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FirstAlbumActivity"
                  android:label="@string/firstType"/>
        <activity android:name=".SecondAlbumActivity"
                  android:label="@string/secondType"/>
        <activity android:name=".ThirdAlbumActivity"
                  android:label="@string/thirdType"/>
        <activity android:name=".FourthAlbumActivity"
                  android:label="@string/fourthType"/>
        <activity android:name=".FifthAlbumActivity"
                  android:label="@string/fifthType"></activity>
    </application>

</manifest>